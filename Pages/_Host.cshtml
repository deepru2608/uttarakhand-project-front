@page "/"
@namespace uttarakhand_project_front.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
    var urlData = Request.Path.Value;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Registration Portal</title>
    <base href="~/" />
    <link rel="icon" href="~/Images/Print-Logo.jpg">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" asp-append-version="true">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet" asp-append-version="true" />
    <link href="CustomCss/bootstrap.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="CustomCss/mdb.min.css" rel="stylesheet" asp-append-version="true" />
    @* <link rel="stylesheet" href="https://cdn.syncfusion.com/ej2/18.1.45/fabric.css" asp-append-version="true" /> *@
    <link rel="stylesheet" href="_content/Syncfusion.Blazor/styles/fabric.css" asp-append-version="true" />
    <link rel="stylesheet" href="CustomCss/mainSite.css" asp-append-version="true" />
    <link rel="stylesheet" href="CustomCss/sb-admin-2.min.css" asp-append-version="true" />
    
    <link href="_content/MatBlazor/dist/matBlazor.css" rel="stylesheet" />
</head>
<body>
    <!-- Blazor overlay -->
    <div id="components-reconnect-modal" class="my-reconnect-modal components-reconnect-hide">
        <div class="show" style="display:none" >
            <p>
                You have left this page now or some connection problem, please wait for page reload to get all functionality back.
            </p>
        </div>
        <div class="failed" style="display:none" >
            <p>
                You have disconnected from server, please wait to get connected from server.
            </p>
        </div>
    </div>
    <app>
        <component type="typeof(App)" render-mode="Server" />
    </app>

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            @* <uttarakhand_project_front.Shared.BackToHome RouteToGo="@urlData" /> *@
            @* An unhandled exception has occurred. See browser dev tools for details. *@
        </environment>
        @* <a class="reload">Reload</a> *@
        @* <a class="dismiss">🗙</a> *@
    </div>


    <script src="CustomJs/jquery.min.js" type="text/javascript" asp-append-version="true"></script>
    <script src="CustomJs/popper.min.js" type="text/javascript" asp-append-version="true"></script>
    <script src="CustomJs/bootstrap.min.js" type="text/javascript" asp-append-version="true"></script>
    <script src="CustomJs/mdb.min.js" type="text/javascript" asp-append-version="true"></script>
    <script src="CustomJS/mainSite.js" asp-append-version="true"></script>
    <script src="CustomJs/sb-admin-2.min.js" asp-append-version="true"></script>
<script src="_content/Microsoft.AspNetCore.ProtectedBrowserStorage/protectedBrowserStorage.js" asp-append-version="true"></script>
@* <script src="CustomJs/reCaptcha.js" asp-append-version="true"></script> *@
<script src="_content/MatBlazor/dist/matBlazor.js"></script>
<script src="_framework/blazor.server.js"></script>
</body>
</html>